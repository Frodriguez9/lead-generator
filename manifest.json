{
    "manifest_version": 3,
    "content_security_policy": {
      "default-source": "self",
      "object-src": "self",
      "script-src": "self"
      },
    "permissions": ["activeTab", "scripting", "storage"],
    "name": "LeadBox",
    "version": "1.2",
    "description": "Save all leads in a single place and share it across multiple devices",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "64": "frontend/images/LeadBox_original_0.5x.png",
          "96": "frontend/images/LeadBox_original_0.75x.png",
          "128": "frontend/images/LeadBox_original_.png"
        }
    },
    "icons": {"128": "frontend/images/LeadBox_original_.png"}
}
